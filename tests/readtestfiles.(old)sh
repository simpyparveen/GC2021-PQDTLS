for f in cf*.txt
do
	#echo "File name is $f"
	while IFS= read -r line
	do
		if [[ "$line" == "Handshake complete with role :DTLS_CLIENT and hs_rtt:"* ]]; then
			echo "$line" | awk '{ print $11 }'
		fi
	done < $f
done






for f in cf*.txt
do
	echo "File name is $f"
	tag=$( tail -n 1 $f )
	#echo $tag
	echo "$tag" | awk '{ print $5 }'
	
done

